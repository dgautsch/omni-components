{
  "name": "omni-components",
  "version": "1.0.0",
  "description": "A web components demonstration with stencil any framework.",
  "main": "dist/index.js",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "module": "dist/esm/index.js",
  "scripts": {
    "start": " browser-sync start --config bs-config.js",
    "test": "jest",
    "build": "stencil build",
    "dev": "stencil build --dev --watch --serve"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dgautsch/omni-components.git"
  },
  "keywords": [
    "JavaScript",
    "TypeScript",
    "Stencil",
    "Web Components"
  ],
  "author": "Dan Gautsch",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dgautsch/omni-components/issues"
  },
  "homepage": "https://github.com/dgautsch/omni-components#readme",
  "dependencies": {
    "@stencil/core": "^0.11.4",
    "@webcomponents/webcomponentsjs": "^2.0.4",
    "bootstrap": "^4.1.3",
    "react": "^16.4.2",
    "vue": "^2.5.17"
  },
  "devDependencies": {
    "@types/jest": "^23.3.1",
    "@stencil/sass": "^0.1.0",
    "browser-sync": "^2.24.6",
    "eslint": "^5.3.0",
    "jest": "^23.4.2"
  },
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/@stencil/core/testing/jest.preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  }
}
